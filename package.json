{
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun --watch elysia.ts\" \"vite\"",
    "lint": "eslint . --ext .ts,.js --cache --fix",
    "format": "prettier --write .",
    "build": "vite build && bun type-check",
    "start": "NODE_ENV=production bun run elysia.ts",
    "db:generate": "bun run scripts/generate-types.ts",
    "db:migrate": "bun run db/migrator.ts up",
    "db:seed": "bun run scripts/seed.ts",
    "migrate": "bun run db:migrate && bun run db:generate && bun run db:seed",
    "migrate:down": "bun run db/migrator.ts down",
    "type-check": "bunx tsc --noemit",
    "start:prod": "bun run db:migrate && bun run db:seed && bun run start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.839.0",
    "@effect/platform": "^0.87.2",
    "@effect/schema": "^0.75.5",
    "@elysiajs/static": "^1.3.0",
    "@elysiajs/trpc": "^1.1.0",
    "@elysiajs/websocket": "^0.2.8",
    "@lit-labs/motion": "^1.0.8",
    "@lit-labs/router": "^0.1.4",
    "@logtail/pino": "^0.5.5",
    "@neondatabase/serverless": "^1.0.1",
    "@preact/signals-core": "^1.10.0",
    "@sinclair/typebox": "^0.34.37",
    "@tiptap/core": "^2.23.1",
    "@tiptap/starter-kit": "^2.23.1",
    "@trpc/client": "^11.4.2",
    "@trpc/server": "^11.4.3",
    "dotenv": "^16.4.5",
    "effect": "^3.16.11",
    "elysia": "^1.3.5",
    "kysely": "^0.28.2",
    "kysely-neon": "^1.1.1",
    "lit": "^3.3.0",
    "motion": "^12.19.2",
    "oslo": "^1.2.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "replicache": "^15.0.1",
    "superjson": "^2.2.2",
    "urlpattern-polyfill": "^10.1.0",
    "vite": "^7.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@kristiandupont/recase": "^1.4.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/ws": "^8.5.11",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "autoprefixer": "^10.4.19",
    "bun-types": "^1.2.17",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.0",
    "eslint-plugin-lit": "^2.1.1",
    "eslint-plugin-lit-a11y": "^5.1.0",
    "eslint-plugin-wc": "^3.0.1",
    "kanel": "^3.14.1",
    "kanel-kysely": "^0.7.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript-eslint": "^8.35.0"
  }
}
